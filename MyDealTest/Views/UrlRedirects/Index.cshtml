@model IEnumerable<MyDealTest.Models.UrlRedirect>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GivenUrl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GeneratedUrl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GivenUrl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GeneratedUrl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedDate)
        </td>
        <td>
            <a href='@Url.Content("~/rd/" + item.GeneratedUrl )'>Go</a> |
            @Html.ActionLink("Details", "Details", new { id=item.UrlRedirecID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UrlRedirecID })
        </td>
    </tr>
}

</table>
